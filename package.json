    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^5.2.2",
    "multer": "^1.3.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "react-google-login": "^3.2.1",
    "reactstrap": "^6.3.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0"
  }
